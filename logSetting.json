{
  "level": "DEBUG",
  "pattern": "%{d(yyyy-MM-dd HH:mm:ss)} -%{5p} %{testProp} %{logger}:%{line} %{msg}",
  "writers": [
    {
      "writerClass": "cj_log.writer.FileWriter",
      "filePath": "./log",
      "fileName": "out.log",
      "writerName": "fileWriter"
    },
    {
      "writerClass": "cj_log.writer.ConsoleWriter",
      "writerName": "consoleWriter"
    }
  ],
  "loggers": [],
  "converter": [
    {
      "converterClass": "cj_log.converter.DefaultConverter",
      "converterKey": "defaultConverter"
    }
  ]
}